{
  "meta": {
    "docsUrl": "https://quasarframework.github.io/quasar-ui-qpdfviewer/docs"
  },
  "props": {
    "src": {
      "type": "String",
      "category": "model",
      "desc": "Source path to the PDF",
      "examples": [
        "src=\"https://www.orimi.com/pdf-test.pdf\""
      ]
    },
    "only-css-zoom": {
      "type": "Boolean",
      "category": "behavior",
      "desc": ""
    },
    "text-layer": {
      "type": "Boolean",
      "category": "behavior",
      "default": "true",
      "desc": ""
    },
    "scale-delta": {
      "type": "Number",
      "category": "behavior",
      "desc": "",
      "default": "1.1",
      "examples": [ "2.0" ]
    },
    "min-scale": {
      "type": "Number",
      "category": "behavior",
      "desc": "",
      "default": "0.5",
      "examples": [ "1.0" ]
    },
    "max-scale": {
      "type": "Number",
      "category": "behavior",
      "desc": "",
      "default": "10.0",
      "examples": [ "5.0" ]
    },
    "scale": {
      "type": ["Number", "String"],
      "category": "behavior",
      "desc": "",
      "default": "auto",
      "examples": [ "" ]
    },
    "single-page": {
      "type": "Boolean",
      "category": "behavior",
      "default": "true",
      "desc": ""
    },
    "mode": {
      "type": "String",
      "category": "model",
      "desc": "",
      "default": "auto",
      "values": ["desktop", "mobile", "auto"],
      "examples": [ "" ]
    },
    "link-target": {
      "type": "String",
      "category": "behavior",
      "desc": "",
      "values": ["none", "self", "blank", "parent", "top"],
      "examples": [ "" ]
    }
  },
  "methods": {
    "prevPage": {
      "desc": "Triggers component to display previous page, if one is available",
      "__exemption": [ "examples" ]
    },
    "nextPage": {
      "desc": "Triggers component to display next page, if one is available",
      "__exemption": [ "examples" ]
    },
    "zoomIn": {
      "desc": "Zooms in on the PDF document",
      "__exemption": [ "examples" ]
    },
    "zoomOut": {
      "desc": "Zooms out on the PDF document",
      "__exemption": [ "examples" ]
    },
    "toggleScrollMode": {
      "desc": "Toggles the scroll mode",
      "__exemption": [ "examples" ]
    },
    "search": {
      "desc": "Performs a search on current PDF document",
      "params": {
        "query": {
          "type": "String",
          "desc": "The string to search for in the current PDF document",
          "examples": ["canada"]
        }
      },
      "__exemption": [ "examples" ]
    },
    "changePage": {
      "desc": "Change to the specified page if it exists",
      "params": {
        "page": {
          "type": "Number",
          "desc": "The page number to change too",
          "examples": ["5"]
        }
      },
      "__exemption": [ "examples" ]
    },
    "openDocument": {
      "desc": "Opens a PDF document",
      "params": {
        "url": {
          "type": "String",
          "desc": "The page number to change too",
          "examples": ["https://www.orimi.com/pdf-test.pdf"]
        }
      },
      "__exemption": [ "examples" ]
    }
  }
}